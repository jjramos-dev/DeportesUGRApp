                <TableRow
                    android:id="@+id/tableRow10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/niniosJubUni"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/niniosJubUni"
                        android:textSize="16sp"
                        android:textStyle="bold" 
                        android:paddingBottom="5dp"
                    	android:paddingLeft="5dp"/>

                    <TextView
                        android:id="@+id/niniosJubNoUni"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/niniosJubNoUni"
                        android:textSize="16sp"
                        android:textStyle="bold" 
                        android:paddingBottom="5dp"
                    	android:paddingLeft="5dp"/>
                </TableRow>

                <TableRow
                    android:id="@+id/tableRow11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/precioNiniosJubFindesUni"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:paddingBottom="5dp"
                    	android:paddingLeft="5dp" />

                    <TextView
                        android:id="@+id/precioNiniosJubFindesNoUni"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:paddingBottom="5dp"
                    	android:paddingLeft="5dp" />
                </TableRow>
            </TableLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="1dip"
                
                android:background="#000000" />

            <Button
                android:id="@+id/button1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/selector"
                android:drawableLeft="@drawable/flecha_negro25prueba"
                android:layout_marginLeft="10dp"
                android:drawablePadding="5dp"
                
                
                android:onClick="onClickMasInfoPiscina"
                android:text="@string/masInfoPiscina" />
            
            <View
        android:layout_width="fill_parent"
		android:layout_height="1dip"
		android:background="#000000"
		 />
            
            <TextView
                        android:id="@+id/nombrePista"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textAppearance="?android:attr/textAppearanceLarge" />
            
            
           
            
        </LinearLayout>
	
	</ScrollView>
	
    </LinearLayout>
    </HorizontalScrollView>
    
