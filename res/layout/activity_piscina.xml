                        android:paddingBottom="5dp"
                    	android:paddingLeft="5dp"/>

                    <TextView
                        android:id="@+id/niniosJubNoUni"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/niniosJubNoUni" 
                        android:textSize="16sp"
                        android:textStyle="bold"
                        android:paddingBottom="5dp"
                    	android:paddingLeft="5dp"/>
                </TableRow>

                <TableRow
                    android:id="@+id/tableRow11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/precioNiniosJubDiarioUni"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:paddingBottom="5dp"
                    	android:paddingLeft="5dp" />

                    <TextView
                        android:id="@+id/precioNiniosJubDiarioNoUni"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="" 
                        android:paddingBottom="5dp"
                    	android:paddingLeft="5dp"/>
                </TableRow>
                
                
                
